        "SSDT0004:DACPAC_HINT_PATH": "warning",
        "SSDT0005:DACPAC_COPY_LOCAL": "warning",
        "SSDT0006:SQL_PROJECT_STRUCT_OPTIONS": "warning",
        "SSDT0007:SQL_PROJECT_OBSOLETE_OPTIONS": "warning",
