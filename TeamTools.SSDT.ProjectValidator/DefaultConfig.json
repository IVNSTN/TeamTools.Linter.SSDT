        "RDS0004:DATASOURCE_NAME_LENGTH": "hint",
        "RDS0005:DATASOURCE_NAME_ALLOWED_SYMBOLS": "warning",
        "RDS0006:DATASOURCE_NAME_NODEFAULT": "warning",
        "RDS0007:DATASOURCE_NAME_TEMPLATE": "hint"
    }
