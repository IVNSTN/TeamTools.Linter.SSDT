        "RDL0010:UNUSED_DATASOURCE": "hint",
        "RDL0011:UNRESOLVED_DATASOURCE_REFERENCE": "error",
        "RDL0012:UNUSED_DATASET": "hint",
        "RDL0013:UNRESOLVED_DATASET_REFERENCE": "error",
        "RDL0014:UNUSED_PARAMETER": "hint",

        "RDS0001:SERVER_LINK_FULLY_QUALIFIED": "warning",
