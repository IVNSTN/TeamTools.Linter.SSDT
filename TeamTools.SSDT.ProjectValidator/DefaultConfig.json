            ".rptproj"
        ],
        "ReportDesign": [
            ".rdl"
        ],
        "ReportDatasource": [
            ".rds"
        ]
    },
    "rules": {
        "SSDT0001:PROJECT_DUP_INCLUDE": "warning",
        "SSDT0002:PROJECT_FOLDER_DANGLING_FILE": "warning",
