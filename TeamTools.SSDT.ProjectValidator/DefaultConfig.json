        "SSDT0009:PROJECT_INCLUDE_ABSOLUTE_PATH": "warning",
        "SSDT0010:PUBLISH_XML_VARIABLE_MISSING": "warning",

        "RDL0001:EMBEDDED_IMAGES": "warning",
